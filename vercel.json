{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "headers": [
    {
      "source": "/soccerProxy",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=86400, stale-while-revalidate" }
      ]
    }
  ]
}
